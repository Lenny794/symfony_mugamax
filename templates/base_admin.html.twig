            <link rel="stylesheet" href="{{ asset('css/font.css') }}">
            <link rel="stylesheet" href="{{ asset('css/indentification.css') }}">
            <link rel="stylesheet" href="{{ asset('css/footer.css') }}">
            <link rel="stylesheet" href="{{ asset('css/page-faq.css') }}">
            <link rel="stylesheet" href="{{ asset('css/page-forum.css') }}">
            <link rel="stylesheet" href="{{ asset('css/checkbox1.css') }}">
            <link rel="stylesheet" href="{{ asset('css/checkbox2.css') }}">
            <link rel="stylesheet" href="{{ asset('css/page-utilisateur.css') }}">
            <link rel="stylesheet" href="{{ asset('css/espace_admin.css') }}">
            <link rel="stylesheet" href="{{ asset('css/page-sécurité.css') }}">
